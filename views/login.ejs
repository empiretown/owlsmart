    <style>
        *,
*::after,
*::before {
    padding: 0;
    margin: 0;
    box-sizing: inherit;
}

html {
    font-size: 62.5%;
    box-sizing: border-box;
}

@media only screen and (max-width: 996px) {
    html {
        font-size: 60%;
    }
}

body {
    font-size: 1.6rem;
    font-family: "Tinos", sans-serif;
    line-height: 1.5;
}

.container {
    max-width: 114rem;
    margin: 0 auto;
    padding: 0 2rem;
}

@media only screen and (max-width: 996px) {
    .container {
        padding: 0 3rem;
    }
}

a {
    text-decoration: none;
}

ul {
    list-style: none;
}

img {
    max-width: 100%;
    /* max-height: 100%; */
}

/* ============ header ============ */
.dropdown .dropdownbtn {
   border: none;
   outline: none;
   color: black;
   font-size: inherit;
   padding: 14px 16px;
   background-color: inherit;
   font-family: inherit;
   margin: 0;
   cursor: pointer;
}

.dropdown-content{
    display: none;
  position: absolute;
  background-color: #f9f9f9;
  min-width: 160px;
  box-shadow: 0px 8px 16px 0px rgba(0,0,0,0.2);
  z-index: 1;
}
.dropdown-content a {
  float: none;
  color: black;
  padding: 12px 16px;
  text-decoration: none;
  display: block;
  text-align: left;
}

.header {
    min-height: 100vh;
    background-color: var(--bg-color);
}

.navigation {
    padding: 1.5rem 0;
}

.nav__center {
    display: flex;
    align-items: center;
    justify-content: space-between;
    width: 100%;
}

.nav__center .nav__menu {
    display: flex;
    align-items: center;
    height: auto;
}

.hamburger {
    display: none;
}

.nav__logo {
    display: flex;
    align-items: center;
}

.nav__logo h1 {
    font-size: 3.5rem;
    color: var(--white);
}

.nav__logo span {
    margin-right: 1.6rem;
}

.nav__logo svg {
    width: 4rem;
    height: 4rem;
    fill: var(--white);
}

.nav__list {
    display: flex;
    align-items: center;
    height: auto;
}

.nav__list .nav__item:not(:last-child) {
    margin-right: 0.5rem;
}

.nav__list .nav__link,
.nav__list .nav__link, icons i {
    display: inline-block;
    padding: 1rem 1.5rem;
    color: var(--white);
    font-weight: 600;
    transition: all 300ms ease-in-out;
}

.nav__list .nav__link:hover {
    color: var(--grey);
}

.nav__list .nav__item:last-child .nav__link {
    padding: 1rem 2rem;
    background-color: var(--default);
    border-radius: 3rem;
    border: 1px solid var(--default);
    transition: all 300ms ease-in-out;
}

.nav__list .nav__item:last-child .nav__link:hover {
    background-color: transparent;
}

@media only screen and (max-width: 768px) {
    .nav__menu {
        justify-content: center;
        position: absolute;
        top: 10%;
        left: 0;
        background-color: var(--black);
        width: 100%;
        max-height: 0;
        overflow: hidden;
        transition: all 300ms ease-in-out;
    }

    .nav__list {
        padding: 1.6rem 0;
        flex-direction: column;
    }

    .hamburger {
        display: block;
        cursor: pointer;
    }

    .hamburger icons i{
        width: 3.5rem;
        height: 3.5rem;
        fill: var(--white);
    }

    .nav__logo h1 {
        font-size: 3rem;
    }

    .nav__logo svg {
        width: 3rem;
        height: 3rem;
        fill: var(--white);
    }

    .nav__list .nav__item:not(:last-child) {
        margin: 0 0 1rem;
    }
}

.banner {
    display: flex;
    align-items: center;
    justify-content: center;
    height: calc(100vh - 8.25rem);
    width: 100%;
}

.banner__center {
    display: flex;
    align-items: center;
}

.banner__right {
    flex: 1 1 30%;
    padding-left: 45px;
    
}

.banner__left {
    color: var(--white);
    margin-right: 2rem;
    flex: 1 1 50%;
}

.banner__left span {
    display: inline-block;
    color: var(--grey);
    margin-bottom: 1.5rem;
}

.banner__left h1 {
    font-size: 4.5rem;
    line-height: 1.2;
    text-transform: capitalize;
    margin-bottom: 1.5rem;
}

.banner__left p {
    color: var(--grey);
    margin-bottom: 3rem;
}

.banner__left a:link,
.banner__left a:visited {
    display: inline-block;
    padding: 1rem 2rem;
    background-color: var(--default);
    color: var(--white);
    font-weight: 600;
    border-radius: 0.5rem;
}

@media only screen and (max-width: 1200px) {
    .banner__left span {
        margin-bottom: 1rem;
    }

    .banner__left h1 {
        font-size: 4rem;
        margin-bottom: 1rem;
    }

    .banner__left p {
        font-size: 1.5rem;
    }
}

@media only screen and (max-width: 996px) {
    .banner__left span {
        font-size: 1.4rem;
    }

    .banner__left h1 {
        font-size: 3rem;
    }

    .banner__left p {
        font-size: 1.4rem;
    }

    .banner__left a:link,
    .banner__left a:visited {
        padding: 0.7rem 1.6rem;
    }
}

@media only screen and (max-width: 768px) {
    .banner__center {
        flex-direction: column;
    }

    .banner__left {
        color: var(--white);
        margin: 0 0 2rem;
        flex: 1 1 50%;
    }

    .banner__left span {
        font-size: 1.3rem;
    }

    .banner__left h1 {
        font-size: 2.5rem;
    }

    .banner__left p {
        font-size: 1.3rem;
    }
}


